	Cheat:toggle()
end, function()
end)









game.CoreGui.ChildAdded:Connect(function(child)
	if child.Name == "Site 55" then
		deleteGui()
	end
end)


-- load
Cheat:SelectPage(Cheat.pages[1], true)if (not game:IsLoaded()) then
