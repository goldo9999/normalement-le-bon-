fireclickdetector(clickdetector)
end)


section1:addButton("P90", function()
	local clickdetector = workspace.gGive.Part.ClickDetector

fireclickdetector(clickdetector)
end)


section1:addButton("IA2", function()
	local clickdetector = workspace.hGive.Part.ClickDetector

fireclickdetector(clickdetector)
end)



local section1 = page1:addSection("Shield")


