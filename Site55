if (not game:IsLoaded()) then
	game.Loaded:Wait();
end



-- init
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/goldo9999/normalement-le-bon-/main/library"))()
local Cheat = library.new("Site 55", 5013109572)


local function deleteGui()
	game.CoreGui:FindFirstChild("Site 55"):Destroy()
end


-- first page
local page1 = Cheat:addPage("Give", 5012544693)
local section1 = page1:addSection("Give gun")



section1:addButton("Revolver", function()
	local clickdetector = workspace.RevolverGive.Part.ClickDetector

fireclickdetector(clickdetector)
end)


section1:addButton("Solaris", function()
	local clickdetector = workspace.Model.aa.Part.ClickDetector

fireclickdetector(clickdetector)
end)


